<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
  
    <TextView
    android:id="@+id/TextVievMagnitude"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
  />
  
    <TextView
    android:id="@+id/TextVievProviderCount"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
  />
    
    <TextView
    android:id="@+id/TextVievCurrentDate"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
  />
    
    <TextView
    android:id="@+id/TextVievDateSelectedItem"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
  />
    
    <TextView
    android:id="@+id/TextVievDateBoolean"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
  />
  
  <ListView
    android:id="@+id/ListViewEarthquake"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
  />
</LinearLayout>

